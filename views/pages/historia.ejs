<%- contentFor('body') %> 

<div class="jumbotron text-center" id = "pageSobre"> 

        <h1>Clique no botão e saiba quem sou eu</h1> 
        <div class="row"> 
                <% for (usuario of usuarios){ %>    
                     <div class="col-sm-4"> 
                        <h2><%= usuario.nome %></h2>  
                        <P><%= usuario.email %></P>
                        <img class="img-responsive" 
                        style="height: 250px" 
                        src="<%= usuario.avatar %>">
                   
                        <button onclick="window.location.href= 'https://github.com/PedroH2' ">Conheça meu perfil</button>
                        </button> 
                     </div> 
                <% } %> 
        </div> 
</div>
